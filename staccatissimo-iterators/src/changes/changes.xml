<document xmlns="http://maven.apache.org/changes/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
  <properties>
    <title>Staccatissimo-Iterators Changes History</title>
    <author>flbulgarelli</author>
  </properties>
  <body>
    <release version="1.0-alpha-3" date="2011-01-30"
      description="Streams">
      <action dev="flbulgarelli" type="add">
        Initial version, with
        empty, undefined, single element, char sequence,
        cons, append
        and
        enumeration iterators
      </action>
    </release>
    <release version="1.0-alpha-4" date="2011-03-11"
      description="Laziness">
      <action dev="flbulgarelli" type="add">
        Adding Thriters and
        Thriterators
      </action>
      <action dev="flbulgarelli" type="add">
        Adding delayed versions of
        cons, single and append iterators
      </action>
    </release>
    <release version="2.1" date="2012-02-21" description="ToString">
      <action dev="flbulgarelli" type="update">
        Adding better toString
        implementations to iterators
      </action>
    </release>
    <release version="2.2" date="2012-03-11" description="Thriters">
      <action dev="flbulgarelli" type="update">
      All iterators implement Thriter interface
      </action>
      <action dev="flbulgarelli" type="add">
        Added UpdateCurrentThriterator
      </action>
    </release>
  </body>
</document>